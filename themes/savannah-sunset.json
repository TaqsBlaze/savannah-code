  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7A6650",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.void",
        "keyword.operator.in",
        "keyword.operator.instanceof",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#E0A458",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Flow Keywords",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#E0A458",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#A3B18A"
      }
    },
    {
      "name": "String Escape Sequences",
      "scope": [
        "constant.character.escape",
        "string.regexp constant.character"
      ],
      "settings": {
        "foreground": "#E5989B"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal",
        "constant.numeric.binary"
      ],
      "settings": {
        "foreground": "#E5989B"
      }
    },
    {
      "name": "Constants & Language Literals",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#E5989B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#D4956B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#588157"
      }
    },
    {
      "name": "Variables — this / self",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#E0A458",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types & Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.enum",
        "entity.name.type.alias",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#5D8AA8"
      }
    },
    {
      "name": "Type Annotations",
      "scope": [
        "meta.type.annotation",
        "entity.name.type.primitive",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#3A7A9A"
      }
    },
    {
      "name": "Inherited / Implemented Class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#5D8AA8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#C2A97A"
      }
    },
    {
      "name": "Decorators / Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#E5989B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#D4A060"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#9A8875"
      }
    },
    {
      "name": "Brackets & Braces",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#A08060"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#E0A458"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#A3B18A"
      }
    },
    {
      "name": "HTML Attribute Values",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#8FA87A"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less"
      ],
      "settings": {
        "foreground": "#C2A97A"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#A3B18A"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.name.tag.scss"
      ],
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "name": "CSS Units & Colors",
      "scope": [
        "keyword.other.unit.css",
        "constant.other.color.rgb-value.css"
      ],
      "settings": {
        "foreground": "#E5989B"
      }
    },
    {
      "name": "Markdown — Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#E0A458",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown — Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "foreground": "#F4A261",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown — Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "foreground": "#E5989B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown — Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.raw.inline"
      ],
      "settings": {
        "foreground": "#A3B18A"
      }
    },
    {
      "name": "Markdown — Links",
      "scope": [
        "markup.underline.link",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#5D8AA8",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown — Blockquote",
      "scope": [
        "markup.quote",
        "punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#7A6650",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown — Lists",
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "markup.list"
      ],
      "settings": {
        "foreground": "#E0A458"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.quoted.double.json meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#C2A97A"
      }
    },
    {
      "name": "JSON Values — Strings",
      "scope": [
        "meta.structure.dictionary.value.json string"
      ],
      "settings": {
        "foreground": "#A3B18A"
      }
    },
    {
      "name": "YAML Keys",
      "scope": [
        "entity.name.tag.yaml",
        "punctuation.separator.key-value.mapping.yaml"
      ],
      "settings": {
        "foreground": "#C2A97A"
      }
    },
    {
      "name": "YAML String Values",
      "scope": [
        "string.unquoted.plain.out.yaml",
        "string.quoted.double.yaml"
      ],
      "settings": {
        "foreground": "#A3B18A"
      }
    },
    {
      "name": "Shell — Variables",
      "scope": [
        "variable.other.normal.shell",
        "variable.other.bracket.shell"
      ],
      "settings": {
        "foreground": "#588157"
      }
    },
    {
      "name": "Shell — Built-in Commands",
      "scope": [
        "support.function.builtin.shell",
        "keyword.other.shell"
      ],
      "settings": {
        "foreground": "#E0A458"
      }
    },
    {
      "name": "Rust — Lifetimes",
      "scope": [
        "entity.name.lifetime.rust",
        "storage.modifier.lifetime.rust",
        "punctuation.definition.lifetime.rust"
      ],
      "settings": {
        "foreground": "#E5989B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Rust — Macros",
      "scope": [
        "entity.name.function.macro.rust",
        "support.function.macro.rust"
      ],
      "settings": {
        "foreground": "#F4A261",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python — Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.decorator.python"
      ],
      "settings": {
        "foreground": "#E5989B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python — Built-ins",
      "scope": [
        "support.type.python",
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#5D8AA8"
      }
    },
    {
      "name": "Go — Built-in functions",
      "scope": [
        "support.function.builtin.go"
      ],
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "name": "Invalid / Errors",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E36414",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#F4A261",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#588157",
    "variable.readonly": "#E5989B",
    "function": "#F4A261",
    "method": "#F4A261",
    "class": "#5D8AA8",
    "interface": "#5D8AA8",
    "type": "#5D8AA8",
    "enum": "#5D8AA8",
    "enumMember": "#E5989B",
    "parameter": "#D4956B",
    "property": "#C2A97A",
    "keyword": "#E0A458",
    "comment": "#7A6650",
    "string": "#A3B18A",
    "number": "#E5989B",
    "decorator": "#E5989B",
    "macro": "#F4A261",
    "lifetime": "#E5989B",
    "selfKeyword": "#E0A458",
    "namespace": "#9DBFCF",
    "typeParameter": "#7DADA4"
  }
}

  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#A08060", "fontStyle": "italic" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#8C5A18", "fontStyle": "bold" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": { "foreground": "#4A7040" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#A05060" }
    },
    {
      "name": "Constants",
      "scope": ["constant.language"],
      "settings": { "foreground": "#A05060", "fontStyle": "italic" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#C8762A" }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#A06030", "fontStyle": "italic" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#3A6030" }
    },
    {
      "name": "Types & Classes",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#2A5A78" }
    },
    {
      "name": "Object Properties",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": { "foreground": "#7A5830" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#A07030" }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#8C5A18" }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#4A7040" }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": { "foreground": "#7A5830" }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css"],
      "settings": { "foreground": "#C8762A" }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": { "foreground": "#8C5A18", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#C8762A", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": { "foreground": "#2A5A78", "fontStyle": "underline" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#C84010", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#3A6030",
    "variable.readonly": "#A05060",
    "function": "#C8762A",
    "method": "#C8762A",
    "class": "#2A5A78",
    "type": "#2A5A78",
    "parameter": "#A06030",
    "property": "#7A5830",
    "keyword": "#8C5A18",
    "comment": "#A08060",
    "string": "#4A7040",
    "number": "#A05060"
  }
}

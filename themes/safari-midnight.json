  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5A4830", "fontStyle": "italic" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#FFB84D", "fontStyle": "bold" }
    },
    {
      "name": "Control Flow",
      "scope": ["keyword.control"],
      "settings": { "foreground": "#FFB84D", "fontStyle": "bold italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": { "foreground": "#7AB880" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#E898B0" }
    },
    {
      "name": "Constants",
      "scope": ["constant.language"],
      "settings": { "foreground": "#E898B0", "fontStyle": "italic" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#FF8040" }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#D06828", "fontStyle": "italic" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#5A9060" }
    },
    {
      "name": "this / self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": { "foreground": "#FFB84D", "fontStyle": "italic" }
    },
    {
      "name": "Types & Classes",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#50A0D0" }
    },
    {
      "name": "Object Properties",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": { "foreground": "#D0A050" }
    },
    {
      "name": "Decorators",
      "scope": ["meta.decorator", "entity.name.function.decorator"],
      "settings": { "foreground": "#E898B0", "fontStyle": "italic" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#E0A040" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": { "foreground": "#7A6040" }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#FFB84D" }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#7AB880" }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": { "foreground": "#D0A050" }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css"],
      "settings": { "foreground": "#FF8040" }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#FFB84D", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": { "foreground": "#50A0D0", "fontStyle": "underline" }
    },
    {
      "name": "Rust Lifetimes",
      "scope": ["entity.name.lifetime.rust", "storage.modifier.lifetime.rust"],
      "settings": { "foreground": "#E898B0", "fontStyle": "italic" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF4500", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#5A9060",
    "variable.readonly": "#E898B0",
    "function": "#FF8040",
    "method": "#FF8040",
    "class": "#50A0D0",
    "type": "#50A0D0",
    "parameter": "#D06828",
    "property": "#D0A050",
    "keyword": "#FFB84D",
    "comment": "#5A4830",
    "string": "#7AB880",
    "number": "#E898B0",
    "decorator": "#E898B0",
    "macro": "#FF8040",
    "lifetime": "#E898B0"
  }
}
